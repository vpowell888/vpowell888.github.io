{
    "name": "ai-diagnostics-platform",
    "version": "1.0.0",
    "description": "Professional diagnostic platform for AI transformation assessments",
    "main": "public/index.html",
    "keywords": [
        "ai",
        "diagnostics",
        "consulting",
        "assessment",
        "transformation"
    ],
    "author": "Your Consulting Firm",
    "license": "PROPRIETARY",
    "scripts": {
        "start": "node scripts/start-static.js",
        "build": "esbuild public/main-module.js --bundle --outfile=public/dist/bundle.js --minify --sourcemap",
        "dev": "esbuild public/main-module.js --bundle --outfile=public/dist/bundle.js --sourcemap --servedir=public --watch",
        "lint": "eslint . --ext .js",
        "test": "jest --runInBand",
        "test:lifecycle": "node tests/full-lifecycle-examples.js",
        "deploy": "firebase deploy --project ceoaitransform --only \"hosting,firestore:rules,firestore:indexes\"",
        "format": "prettier --write \"**/*.{js,json,md}\"",
        "prepare": "npx husky install"
    },
    "devDependencies": {
        "esbuild": "^0.19.4",
        "eslint": "^8.48.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.5",
        "firebase-tools": "^12.9.1",
        "http-server": "^14.1.1",
        "jest": "^29.6.0",
        "prettier": "^3.8.0",
        "puppeteer": "^24.37.0"
    },
    "dependencies": {
        "jest-environment-jsdom": "^30.2.0",
        "picomatch": "4.0.3"
    }
}
